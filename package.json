  "private": true,
  "scripts": {
    "clear": "jest --clearCache",
    "test": "jest --config jest.config.ts --no-stack-trace --colors ",
    "test:watch": "jest --config jest.config.ts --watch --no-stack-trace",
    "test:ci": "jest --config jest.config.ts --ci --json --outputFile=test-results.json --no-stack-trace",
    "test:summary": "jest --config jest.config.ts --no-stack-trace --reporters=jest-summary-reporter",
    "test:coverage": "jest --config jest.config.ts --coverage --no-stack-trace",
    "test:perf": "jest --config jest.config.ts --no-stack-trace --detectOpenHandles --logHeapUsage --colors",
    "test:specific": "jest --config jest.config.ts --testPathPattern",
    "test:consistency": "jest --config jest.config.ts --testPathPattern=subscription-consistency",
    "validate-styles": "node scripts/validate-styles.js",
    "dev": "npm run validate-styles && next dev ",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "server": "node server",
    "postinstall": "prisma generate",
    "dev:migrate": "npx prisma migrate dev",
    "prod:migrate": "npx prisma migrate deploy",
    "prod:start": "set NODE_ENV=production && node start",
    "export": "next export",
    "prod:build": "npm run prod:migrate && npm run build",
    "knip": "knip",
    "prod": "set NODE_ENV=production npm run migrate && npm run build && npm run start",
    "check-consistency": "tsx scripts/check-subscription-consistency.ts",
    "fix-consistency": "tsx scripts/check-subscription-consistency.ts --fix",
    "setup-consistency": "npx prisma db execute --file prisma/migrations/fix_subscription_consistency.sql",
    "video-fix": "node scripts/video-health-check.js"
  },
