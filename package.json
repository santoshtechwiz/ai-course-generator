    "quality:ci": "tsx scripts/code-quality-check.ts --ci",
    "test:quality": "npm run lint && npm run test && npm run quality",
    "test:coverage": "vitest --coverage",
    "docs:check": "tsx scripts/check-documentation.ts",
    "pre-commit": "npm run lint:fast && npm run test && npm run quality:ci",
    "pre-push": "npm run test:coverage && npm run docs:check"
